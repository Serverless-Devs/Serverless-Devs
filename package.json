{
  "name": "@serverless-devs/s",
  "version": "2.1.14",
  "description": "Serverless devs tool, serverless developer tool, supports Alibaba cloud, AWS, azure, baidu cloud, Huawei cloud, Google cloud and Tencent cloud.",
  "homepage": "https://www.serverless-devs.com",
  "keywords": [
    "serverless",
    "alibaba",
    "tencent",
    "azure",
    "baidu",
    "huawei",
    "google",
    "function",
    "faas",
    "serverless-devs"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serverless-Devs/Serverless-Devs"
  },
  "bugs": {
    "url": "https://github.com/Serverless-Devs/Serverless-Devs/issues"
  },
  "scripts": {
    "start": "npm run watch",
    "format": "prettier --write src/",
    "prewatch": "rm -rf lib/* && cp -r ./src/daemon ./lib",
    "watch": "tsc --skipLibCheck -w",
    "prebuild": "rm -rf lib/*",
    "build": "tsc && cp -r ./src/daemon ./lib",
    "postbuild": "esbuild lib/index.js --bundle --log-level=error --minify --external:@serverless-devs/core --external:@serverless-devs/ui --platform=node --format=cjs --target=node10.4 --allow-overwrite --outfile=lib/index.js",
    "beta": "npm publish --tag=beta",
    "pkg": "tsc && pkg bin/s"
  },
  "main": "./lib/index.js",
  "bin": {
    "s": "bin/s"
  },
  "devDependencies": {
    "@serverless-devs/credential": "beta",
    "@serverless-devs/engine": "^0.0.1-beta.2",
    "@serverless-devs/load-application": "beta",
    "@serverless-devs/logger": "beta",
    "@serverless-devs/parse-spec": "beta",
    "@serverless-devs/registry": "beta",
    "@serverless-devs/utils": "beta",
    "@types/node": "^18.16.1",
    "chalk": "^4.1.0",
    "commander": "^11.0.0",
    "global-agent": "^3.0.0",
    "inquirer": "8.2.5",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "table-layout": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  }
}
